{
  "schemaVersion": 1,
  "id": "nostringsattached",
  "version": "${version}",

  "name": "No Strings Attached",
  "description": "Hide tripwire on the ground when not holding string",
  "authors": [
    "iGalaxy"
  ],
  "contact": {
    "homepage": "https://igalaxy.dev",
    "sources": "https://github.com/iGalaxyYT/no-strings-attached"
  },

  "license": "MIT",

  "icon": "assets/nostringsattached/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.igalaxy.nostringsattached.NoStringsAttached"
      }
    ],
    "modmenu": [
      "dev.igalaxy.nostringsattached.ModMenuIntegration"
    ]
  },
  "mixins": [
    "nostringsattached.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.10",
    "fabric": "*",
    "minecraft": ">=1.19.2",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.8.5+kotlin.1.7.20",
    "cloth-config2": ">=8.0.0"
  },
  "suggests": {
    "modmenu": ">=4.0.0"
  }
}
